                                            <button class="btn btn-default plus" type="button"
                                                    v-on:keydown.esc.stop.prevent="closeBox()"
                                                    v-on:click.stop="(children++ || true) && checkChildren()">
                                                +
                                            </button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="travellersandcabin-row">
                                <div class="col-xs-4 col-sm-5 col-md-6">
                                    <label>Beb&eacute;s</label>
                                </div>
                                <div class="col-xs-8 col-sm-7 col-md-6">
                                    <div class="input-group">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default minus" type="button"
                                                    v-on:keydown.esc.stop.prevent="closeBox()"
                                                    v-on:click.stop="(babies-- || true) && checkBabies()">
                                                -
                                            </button>
                                        </span>
                                        <input type="text" class="form-control travellersandcabin-input"
                                               v-on:change="checkBabies()"
                                               v-on:keydown.enter.stop.prevent
                                               v-on:keydown.esc.stop.prevent="closeBox()"
                                               v-on:click.stop
                                               v-model="babies">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default plus" type="button"
                                                    v-on:keydown.esc.stop.prevent="closeBox()"
                                                    v-on:click.stop="(babies++ || true) && checkBabies()">
                                                +
                                            </button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="travellersandcabin-row">
                                <div class="col-xs-4">
                                    <label>Clase</label>
                                </div>
                                <div class="col-xs-8">
                                    <select class="form-control" v-model="cabin"
                                            v-on:keydown.esc.stop.prevent="closeBox()"
                                            v-on:click.stop
                                            v-on:keydown.tab="checkTabKey($event)">
                                        <option value="0">Turista</option>
                                        <option value="1">Business</option>
                                        <option value="2">Primera</option>
                                    </select>
                                </div>
                            </div>
                        </div>
